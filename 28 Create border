def drawBorder(width,height):
    if width <= 2 or height <= 2:
        return
    print('+' + '-'*(width-2) + '+') #Prints top border
    for h in range(2, height): #in-fills the middle side borders
        print('|' + ' '*(width-2) + '|')
    print('+' + '-'*(width-2) + '+') #Prints bottom border
    
drawBorder(2,10)

