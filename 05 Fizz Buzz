def fizzbuzz(upTo):
    for number in range(1, upTo + 1):
        if number % 3 == 0:
            if number % 5 == 0: # checks for 15-divisibility
                print("FizzBuzz ", end='')
            else:
                print("Fizz ", end='')
        elif number % 5 == 0:
            print("Buzz ", end='')
        else:
            print(number, "", end='')
    

fizzbuzz(35)
